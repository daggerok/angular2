{
  "name": "angular2-master",
  "version": "0.0.3",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "sleep": "sleep 3",

    "precompile": "rimraf -rf ./dist",
    "compile": "webpack --config=./config/webpack.config.js",

    "ghpages": "npm run compile -- --env ghpages",

    "serve": "npm-run-all build start-server",
    "build": "npm run compile -- --env production",
    "start-server": "browser-sync start -c ./config/browser-sync/bs-config.js",
    "server": "npm run start-server -- --no-notify --no-open --no-online --no-inject-changes --no-reload-on-restart",

    "open": "open-url localhost:8000",
    "open-url": "npm-run-all sleep open",
    "start": "npm-run-all -p webpack-dev-server open-url",
    "webpack": "npm run compile -- --env development",
    "webpack-dev-server": "webpack-dev-server -w -d --config=./config/webpack.config.js --env development",

    "lint": "tslint -t verbose -c ./config/tslint.json 'src/*.ts' 'src/**/*.ts' 'e2e/*.ts' 'e2e/**/*.ts'",

    "pretest": "npm run webpack",
    "test": "concurrently 'npm run protractor' 'npm run server' --kill-others --success first -p '[{name}]' -n 'E2E,HTTP' -c 'green,cyan' # colors: black, red, green, yellow, blue, magenta, cyan, white, gray",
    "protractor": "protractor ./config/protractor.js",
    "preprotractor": "npm-run-all sleep webdriver-manager-update",
    "webdriver-manager-update": "webdriver-manager update",

    "compose": "docker-compose --project-name=react -f ./config/nginx/docker-compose.yml",
    "pre-compose-up": "npm run build",
    "compose-up": "npm run docker-compose -- up -d --remove-orphans",
    "compose-down": "npm run docker-compose -- down --remove-orphans --rmi local -v",

    "docker": "npm-run-all build docker:*",
    "docker:rm": "docker rm -f daggerok-angular2-master | true",
    "docker:rmi": "docker rmi -f daggerok/angular2:master | true",
    "docker:build": "docker build --rm -t daggerok/angular2:master -f config/nginx/Dockerfile .",
    "docker:tag": "docker tag daggerok/angular2:master daggerok/angular2:master-$(date +%Y-%m-%d-%H)",
    "docker:push": "docker push daggerok/angular2:master-$(date +%Y-%m-%d-%H)",
    "docker:tag-latest": "docker tag daggerok/angular2:master-$(date +%Y-%m-%d-%H) daggerok/angular2:master",
    "docker:push-latest": "docker push daggerok/angular2:master"
  },
  "pre-push": [
    "lint",
    "test",
    "build"
  ],
  "dependencies": {
    "@angular/common": "2.4.9",
    "@angular/core": "2.4.9",
    "@angular/forms": "2.4.9",
    "@angular/http": "2.4.9",
    "@angular/platform-browser-dynamic": "2.4.9",
    "@angular/router": "3.4.9",
    "@types/core-js": "0.9.36",
    "bootstrap": "3.3.7",
    "es6-promise": "4.1.0",
    "es6-shim": "0.35.3",
    "ie-shim": "0.1.0",
    "jquery": "3.1.1",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.2.0",
    "zone.js": "0.8.1"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.2",
    "@angular/compiler": "2.4.9",
    "@angular/compiler-cli": "2.4.9",
    "@angular/platform-browser": "2.4.9",
    "@angular/platform-server": "2.4.9",
    "@angular/upgrade": "2.4.9",
    "@types/chai": "3.4.35",
    "@types/jasmine": "2.5.45",
    "@types/node": "7.0.7",
    "@types/selenium-webdriver": "3.0.0",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "2.2.11",
    "@ultimate/aot-loader": "0.1.12",
    "angular2-router-loader": "0.3.5",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "6.7.7",
    "awesome-typescript-loader": "3.1.2",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-0": "6.22.0",
    "base-href-webpack-plugin": "1.0.2",
    "browser-sync": "2.18.8",
