    "@types/core-js": "^0.9.34",
    "bootstrap": "^3.3.7",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.1",
    "ie-shim": "^0.1.0",
    "jquery": "^3.1.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-beta.12",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@angular/platform-server": "^2.2.1",
    "@types/chai": "3.4.34",
