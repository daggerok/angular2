{
  "name": "socket-chat",
  "version": "0.0.1",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "sleep": "sleep 3",
    "clean": "rimraf -rf ./dist",
    "prebuild": "npm run clean",
    "prestart": "npm i",
    "predev": "npm i",
    "start-socket-server": "nodemon ./server/index.js",
    "start-server": "browser-sync start -c ./config/bs-config.js",
    "watch": "npm run webpack -- -w",
    "webpack": "NODE_ENV=development webpack --config=./config/webpack/dev.babel.js -c --process",
    "dev": "npm-run-all -p watch start-server",
    "open-dev": "open-url localhost:8000",
    "open-dev-url": "npm-run-all sleep open-dev",
    "webpack-dev-server": "NODE_ENV=development webpack-dev-server -w -d --config=./config/webpack/dev.babel.js --content-base=./src",
    "start.ui": "npm-run-all -p webpack-dev-server open-dev-url",
    "start": "npm-run-all -p webpack-dev-server",
    "open-prod": "open-url localhost:8080",
    "open-prod-url": "npm-run-all sleep open-prod",
    "start-server-and-open-prod-url": "npm-run-all -p open-prod-url",
    "build": "NODE_ENV=production webpack --config=./config/webpack/prod.babel.js -c --progress --profile --bail -p",
    "gh-pages": "NODE_ENV=production webpack --config=./config/webpack/gh-pages.babel.js -c --progress --profile --bail -p",
    "serve": "npm-run-all build start-server",
    "lint": "tslint 'src/**/*.ts' -c ./config/tslint.json --noUnusedParameters --noUnusedLocals",
    "test": "karma start ./config/karma/karma.conf.js --single-run",
    "tdd": "karma start ./config/karma/karma.conf.js",
    "e2e": "npm-run-all -p prepare-e2e wait-20s-and-run-protractor",
    "pree2e": "webdriver-manager update",
    "prepare-e2e": "concurrently -p '[{name}]' -n 'HTTP' -c 'bgYellow.bold' 'npm run webpack-dev-server' --kill-others --success first",
    "wait-20s-and-run-protractor": "concurrently -p '[{name}]' -n 'PROTRACTOR' -c 'bgGreen.bold' 'npm-run-all wait-20s protractor' --kill-others --success first",
    "postwait-20s-and-run-protractor": "npm run stop",
    "wait-20s": "sleep 25",
    "protractor": "protractor ./config/protractor.js",
    "stop": "kill -9 $(ps waux | grep webpack-dev-server | grep -v color | awk '{print $2}') | true"
  },
  "dependencies": {
    "@angular/common": "^2.2.1",
    "@angular/compiler": "^2.2.1",
    "@angular/compiler-cli": "^2.2.1",
    "@angular/core": "^2.2.1",
    "@angular/forms": "^2.2.1",
    "@angular/http": "^2.2.1",
    "@angular/platform-browser": "^2.2.1",
    "@angular/platform-browser-dynamic": "^2.2.1",
