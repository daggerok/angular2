    "webpack-dev-middleware": "1.10.1",
    "webpack-dev-server": "2.4.1"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "syntax-dynamic-import",
      "transform-class-properties"
    ]
  },
  "keywords": [],
  "author": "Maksim Kostromin <daggerok@gmail.com>",
  "repository": {
    "type": "git",
