{
  "name": "rb",
  "version": "0.0.1",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "postinstall": "npm-run-all typings:*",
    "typings:jquery": "typings install dt~jquery --save --global",
    "build": "npm run compile -- --prod",
    "compile": "ng build",
    "json-server": "babel-node ./node_modules/.bin/json-server --watch -c ./config/json-server/config.json ./config/json-server/testdata.babel.js",
    "ng-serve": "ng serve --port 3000 --proxy-config ./config/proxy/api.json",
    "start": "npm-run-all -p json-server ng-serve",
    "dev": "ng test",
    "pree2e-manual": "webdriver-manager update",
    "e2e-manual": "protractor",
    "wait-20s": "sleep 20",
    "lint": "tslint 'src/**/*.ts'",
    "stop": "killall -9 angular-cli",
    "webdriver": "webdriver-manager update",
    "watch": "karma start ./config/karma/watch.js",
    "test": "karma start ./config/karma/singlerun.js",
    "prepare-e2e": "(npm-run-all -p start webdriver) &",
    "protractor": "protractor ./config/protractor/conf.js",
    "e2e": "npm-run-all -p prepare-e2e wait-20s-and-run-e2e",
    "travis-e2e": "npm-run-all -p prepare-e2e wait-20s-and-run-e2e",
    "wait-20s-and-run-e2e": "exit 0 | npm-run-all wait-20s protractor stop",
    "stop-old-fashon": "kill -9 $(ps waux | grep angular-cli | grep webdriver | grep -v color | awk '{print $2}')"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "@types/jquery": "^2.0.33",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "jquery": "^3.1.1",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.18",
    "babel-cli": "^6.16.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "codelyzer": "1.0.0-beta.1",
    "faker": "^3.1.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "json-server": "^0.8.22",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "npm-run-all": "^3.1.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "typings": "^1.4.0"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "description": "This project was generated with [angular-cli](https://github.com/angular/angular-cli) version 1.0.0-beta.18.",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/angular2.git"
  },
  "keywords": [
    "Angular2"
  ],
  "author": "Maksim Kostromin <daggerok@gmail.com>",
  "bugs": {
    "url": "https://github.com/daggerok/angular2/issues"
  },
  "homepage": "https://github.com/daggerok/angular2#readme"
}
