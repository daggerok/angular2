{
  "name": "ng4-angular-material",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8000",
    "precompile": "rimraf -rf ./dist",
    "compile": "ng build",
    "build": "npm run compile -- --ec --prod --aot -vc",
    "ghpages": "npm run build -- --base-href=/angular2/",
    "preserve": "npm run build",
    "serve": "lite-server",
    "lint": "npm-run-all -p lint:*",
    "lint:ng": "ng lint",
    "lint:src": "tslint --type-check --project src/tsconfig.app.json",
    "lint:e2e": "tslint --type-check --project e2e/tsconfig.e2e.json",
    "test": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": ">=4.0.0-beta <5.0.0",
    "@angular/compiler": ">=4.0.0-beta <5.0.0",
    "@angular/core": ">=4.0.0-beta <5.0.0",
    "@angular/forms": ">=4.0.0-beta <5.0.0",
    "@angular/http": ">=4.0.0-beta <5.0.0",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": ">=4.0.0-beta <5.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0-beta <5.0.0",
    "@angular/router": ">=4.0.0-beta <5.0.0",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "material-design-icons": "3.0.1",
    "normalize.css": "5.0.0",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": ">=4.0.0-beta <5.0.0",
    "@types/core-js": "0.9.35",
    "@types/hammerjs": "2.0.34",
    "@types/jasmine": "2.5.43",
    "@types/node": "7.0.5",
    "codelyzer": "3.0.0-beta.3",
    "concurrently": "3.4.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "lite-server": "2.2.2",
    "npm-run-all": "4.0.2",
    "protractor": "5.1.1",
    "rimraf": "2.6.1",
    "ts-node": "2.1.0",
    "tslint": "4.5.1",
    "typescript": "2.2.1"
  }
