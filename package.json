{
  "name": "ng4wp2",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --env development",

    "precompile": "rimraf -rf ./dist",
    "compile": "webpack",

    "build": "npm run compile -- -p --env production",
    "ghpages": "npm run compile -- -p --env ghpages",

    "preserve": "npm run build",
    "serve": "concurrently --kill-others 'npm run lite-server'",
    "lite-server": "concurrently --kill-others 'lite-server -c bs-config.js'",

    "test": "karma start ./karma.conf.js --singleRun",

    "lint": "npm-run-all -p lint:*",
    "lint:src": "tslint --type-check --project src/tsconfig.app.json",
    "lint:e2e": "tslint --type-check --project e2e/tsconfig.e2e.json",

    "pree2e": "npm run compile -- --env development",
    "e2e": "concurrently --success first 'npm run protractor' 'npm run browser-sync' --kill-others -p '[{name}]' -n 'E2E,HTTP' -c 'green,cyan'",
    "protractor": "protractor ./protractor.conf.js",
    "preprotractor": "webdriver-manager update --standalone false --gecko false --quiet",
    "browser-sync": "browser-sync start --config bs-config.js --port=4200 --no-notify --no-open --no-online --no-inject-changes --no-reload-on-restart"
  },
  "private": true,
  "dependencies": {
    "@angular/common": ">=4.0.0-beta <5.0.0",
    "@angular/compiler": ">=4.0.0-beta <5.0.0",
    "@angular/core": ">=4.0.0-beta <5.0.0",
    "@angular/forms": ">=4.0.0-beta <5.0.0",
    "@angular/http": ">=4.0.0-beta <5.0.0",
    "@angular/platform-browser": ">=4.0.0-beta <5.0.0",
    "@angular/platform-browser-dynamic": ">=4.0.0-beta <5.0.0",
    "@angular/router": ">=4.0.0-beta <5.0.0",
    "core-js": "2.4.1",
    "font-awesome": "4.7.0",
    "normalize.css": "5.0.0",
    "primeng": "2.0.2",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": ">=4.0.0-beta <5.0.0",
    "@types/core-js": "0.9.35",
    "@types/jasmine": "2.5.43",
    "@types/node": "7.0.5",
    "autoprefixer": "6.7.6",
    "codelyzer": "3.0.0-beta.3",
    "concurrently": "^3.4.0",
    "css-loader": "0.26.2",
    "cssnano": "3.10.0",
    "exports-loader": "0.6.4",
    "file-loader": "0.10.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "2.2.3",
    "lite-server": "2.2.2",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "postcss-url": "5.1.2",
    "protractor": "5.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.2",
    "script-loader": "0.7.0",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.2",
    "stylus-loader": "2.5.0",
    "ts-node": "2.1.0",
    "tslint": "4.5.1",
    "typescript": "2.2.1",
    "url-loader": "0.5.8",
    "webpack-dev-server": "2.4.1"
  }
}
