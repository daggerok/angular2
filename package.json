{
  "name": "material2",
  "version": "0.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "preprotractor": "sleep 20",
    "pree2e": "webdriver-manager update",
    "e2e": "npm-run-all -p start protractor",
    "stop": "kill -9 $(ps waux| grep angular-cli| grep -v color| awk '{print $2}') | true",
    "protractor": "concurrently -p '[{name}]' -n 'E2E' -c 'bgGreen.bold' 'protractor' --kill-others --success first",
    "compile": "ng build",
    "build": "ng build -- --prod",
    "start": "ng serve --port 8000",
    "lint": "tslint 'src/**/*.ts' --noUnusedParameters --noUnusedLocals",
    "test": "npm run e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.1.2",
    "@angular/compiler": "~2.1.2",
    "@angular/core": "~2.1.2",
    "@angular/forms": "~2.1.2",
    "@angular/http": "~2.1.2",
    "@angular/material": "~2.0.0-alpha.11-3",
    "@angular/platform-browser": "~2.1.2",
    "@angular/platform-browser-dynamic": "~2.1.2",
    "@angular/router": "~3.1.0",
    "@angular2-material/card": "^2.0.0-alpha.8-experimental3",
    "@angular2-material/checkbox": "^2.0.0-alpha.8-experimental3",
    "@angular2-material/core": "^2.0.0-alpha.8-experimental3",
    "@angular2-material/icon": "^2.0.0-alpha.8-experimental3",
    "@angular2-material/input": "^2.0.0-alpha.8-experimental3",
    "@angular2-material/list": "^2.0.0-alpha.8-experimental3",
    "@angular2-material/toolbar": "^2.0.0-alpha.8-experimental3",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "~0.6.26"
  },
  "devDependencies": {
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.19-3",
    "codelyzer": "2.0.0-beta.3",
    "concurrently": "^3.1.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "npm-run-all": "^3.1.1",
    "protractor": "4.0.9",
    "rimraf": "^2.5.4",
    "ts-node": "1.2.1",
    "tslint": "4.0.2",
    "typescript": "2.1.1",
    "webdriver-manager": "10.2.5"
  }
}
