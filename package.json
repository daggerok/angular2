{
  "name": "angular2",
  "version": "0.0.1",
  "license": "ISC",
  "angular-cli": {},
  "scripts": {
    "typings-disabled-becouse-of-@types/*:core-js": "typings install dt~core-js --save --global",
    "typings-disabled-becouse-of-@types/*:node": "typings install dt~node --save --global",
    "typings-disabled-becouse-of-@types/*:jquery": "typings install dt~jquery --save --global",
    "typings-disabled-becouse-of-@types/*:jasmine": "typings install dt~jasmine --save --global",
    "typings:no-typings": "echo 'no typings for install..'",
    "postinstall": "npm-run-all typings:*",
    "json-server": "babel-node ./node_modules/.bin/json-server --watch -c ./config/json-server/config.json ./config/json-server/testdata.babel.js",
    "webpack": "ENV=development webpack --config=./config/webpack/dev.babel.js -c --process",
    "clean": "rimraf -rf ./dist ./typings*",
    "sleep": "sleep 3",
    "dev": "npm-run-all -p watch json-server",
    "watch": "npm run webpack -- -w",
    "predev": "npm run clean",
    "webpack-dev-server": "ENV=development webpack-dev-server -w -d --config=./config/webpack/dev.babel.js --content-base=./src",
    "open-dev-url": "npm-run-all sleep open-dev",
    "open-dev": "open-url localhost:8000",
    "serve": "npm-run-all build start-server-and-open-prod-url",
    "build": "ENV=production webpack --config=./config/webpack/prod.babel.js -c --progress --profile --bail -p",
    "start-server-and-open-prod-url": "npm-run-all -p open-prod-url json-server",
    "open-prod-url": "npm-run-all sleep open-prod",
    "open-prod": "open-url localhost:8080",
    "prebuild": "npm run clean",
    "wait-20s": "sleep 20",
    "killall": "killall -9 node",
    "old-start": "npm-run-all -p webpack-dev-server json-server open-dev-url",
    "old-lint": "tslint 'src/**/*.ts'",
    "old-test": "karma start ./config/karma/singlerun.js",
    "old-e2e": "npm-run-all -p prepare-e2e wait-20s-and-run-e2e",
    "webdriver": "webdriver-manager update",
    "tdd": "karma start ./config/karma/watch.js",
    "protractor": "protractor ./config/protractor/conf.js",
    "travis-e2e": "npm-run-all -p prepare-e2e wait-20s-and-run-e2e",
    "prepare-e2e": "(npm-run-all -p webpack-dev-server webdriver) &",
    "wait-20s-and-run-e2e": "exit 0 | npm-run-all wait-20s protractor stop",

    "start": "tsc && concurrently 'tsc -w' 'lite-server'",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently 'http-server -s' 'protractor protractor.config.js' --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "test": "tsc && concurrently 'tsc -w' 'karma start karma.conf.js'",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "webdriver:update": "webdriver-manager update",

    "stop": "kill -9 $(ps waux | grep webpack-dev-server | grep webdriver | grep -v color | awk '{print $2}')"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.1.1",
    "@angular/compiler": "~2.1.1",
    "@angular/core": "~2.1.1",
    "@angular/forms": "~2.1.1",
    "@angular/http": "~2.1.1",
    "@angular/platform-browser": "~2.1.1",
    "@angular/platform-browser-dynamic": "~2.1.1",
    "@angular/router": "~3.1.1",
    "@angular/upgrade": "~2.1.1",

    "angular-in-memory-web-api": "~0.1.13",
    "bootstrap": "^3.3.7",
    "systemjs": "0.19.39",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.26",
    "@types/core-js": "^0.9.34",
    "es6-promise": "^4.0.5",
    "ie-shim": "^0.1.0",
    "jquery": "^3.1.1",
    "ts-helpers": "^1.1.1"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.3",

    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "tslint": "^3.15.1",
    "lodash": "^4.16.2",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^4.0.9",
    "rimraf": "^2.5.4",

    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.45",
    "@types/jasmine": "^2.5.35",
    "@types/selenium-webdriver": "^2.53.32",

    "@angular/compiler": "^2.1.0",
    "@angular/compiler-cli": "^2.1.1",
    "@angular/platform-server": "^2.1.0",
    "@types/hammerjs": "^2.0.33",
    "@types/source-map": "^0.1.27",
    "@types/uglify-js": "^2.0.27",
    "@types/webpack": "^1.12.34",
    "angular-cli": "^1.0.0-beta.18",
    "angular2-template-loader": "^0.5.0",
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.5.0",
    "awesome-typescript-loader": "^2.2.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "*",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "codelyzer": "^0.0.26",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "editorconfig": "^0.13.2",
    "es6-shim": "^0.35.1",
    "eslint": "^3.8.1",
    "eslint-config-stylelint": "^5.0.0",
    "eslint-config-xo": "^0.17.0",
    "eslint-plugin-react": "^6.4.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "husky": "^0.11.9",
    "imports-loader": "^0.6.5",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-spec-reporter": "2.7.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.8.22",
    "karma-coverage": "^1.1.1",
    "karma-mocha-reporter": "^2.0.0",
    "karma-remap-coverage": "^0.1.1",
    "karma-remap-istanbul": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "npm-run-all": "^3.1.1",
    "open-url": "^2.0.2",
    "parse5": "^2.2.2",
    "postcss-loader": "^1.0.0",
    "reflect-metadata": "^0.1.8",
    "resolve-url-loader": "^1.6.0",
    "script-ext-html-webpack-plugin": "^1.3.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "1.1.2",
    "ts-node": "^1.3.0",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.5.0",
    "typings": "^1.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.16.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.15.0"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "keywords": [],
